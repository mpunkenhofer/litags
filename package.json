{
  "name": "litags",
  "version": "0.0.0",
  "description": "A browser extension that enables you to assign tags to players on lichess.org.",
  "homepage": "https://github.com/mpunkenhofer/litags",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --mode=production --display-error-details",
    "predebug": "rimraf dist",
    "debug": "cross-env NODE_ENV=development webpack --config webpack.dev.config.js --mode=development --watch --display-error-details",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@types/chrome": "0.0.91",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.17",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-core": "^6.26.3",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fast-css-loader": "^1.0.2",
    "fast-sass-loader": "^1.5.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "ignore-emit-webpack-plugin": "^2.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "redux-logger": "^3.0.6",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webextension-polyfill-ts": "^0.12.0",
    "webpack": "^4.41.2",
    "webpack-chrome-extension-reloader": "^1.3.0",
