{
  "name": "litags",
  "private": true,
  "version": "0.0.0",
  "description": "A browser extension that enables you to assign tags to players on lichess.org.",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --mode=production --display-error-details",
    "predebug": "rimraf dist",
    "debug": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 webpack --config webpack.dev.config.js --mode=development --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@types/chrome": "0.0.91",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "fast-css-loader": "^1.0.2",
    "fast-sass-loader": "^1.5.0",
    "file-loader": "^4.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "sortablejs": "^1.10.1",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "tslint-webpack-plugin": "^2.1.0",
    "typescript": "^3.6.4",
    "webextension-polyfill": "^0.5.0",
    "webpack": "^4.41.2",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-extension-reloader": "^1.1.1"
  }
}
