{
  "name": "litags",
  "version": "1.0.0",
  "description": "A browser extension that enables you to assign tags to players on lichess.org.",
  "homepage": "https://github.com/mpunkenhofer/litags",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "build": "webpack --config webpack.production.js",
    "prewatch": "rimraf dist",
    "watch": "webpack --config webpack.development.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "icons": "py ./bin/svg2pngs.py ./assets/images/litags_icon.svg -d assets/images/ -p litags_icon -r 16 32 48 64 96 128"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.17",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fast-css-loader": "^1.0.2",
    "fast-sass-loader": "^1.5.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "ignore-emit-webpack-plugin": "^2.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webextension-polyfill-ts": "^0.12.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.9.0",
    "webpack-extension-manifest-plugin": "^0.5.0",
    "webpack-extension-reloader": "^1.1.4",
    "webpack-merge": "^4.2.2",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.5",
    "array-move": "^2.2.1",
    "bootstrap": "^4.4.1",
    "focus-trap-react": "^6.0.0",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-color": "^2.18.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.10.1",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.3"
  }
