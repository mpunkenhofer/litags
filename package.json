{
  "name": "litags",
  "private": true,
  "version": "0.0.0",
  "description": "A browser extension that enables you to assign tags to players on lichess.org.",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --mode=production",
    "prestart": "rimraf dist",
    "start": "cross-env NODE_ENV=development webpack --config webpack.dev-ext-reloader.config.js --mode=development --watch"
  },
  "devDependencies": {
    "@types/chrome": "0.0.91",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "tslint-webpack-plugin": "^2.1.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
