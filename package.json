    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-color": "^2.18.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.10.1",
    "uuid": "^3.3.3"
